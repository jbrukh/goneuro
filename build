# gb provides configuration-free building and distributing

echo "Build script generated by gb: http://go-gb.googlecode.com" 
if [ "$1" = "goinstall" ]; then
echo Running goinstall \

else
echo Building \
&& echo "(in pkg)" gomake $1 && cd pkg && gomake $1 && cd - > /dev/null \
&& echo "(in cmd/examples/abort)" gomake $1 && cd cmd/examples/abort && gomake $1 && cd - > /dev/null \
&& echo "(in cmd/examples/streamer)" gomake $1 && cd cmd/examples/streamer && gomake $1 && cd - > /dev/null \
&& echo "(in cmd/tosocket)" gomake $1 && cd cmd/tosocket && gomake $1 && cd - > /dev/null \

fi

# The makefiles above are invoked in topological dependence order
